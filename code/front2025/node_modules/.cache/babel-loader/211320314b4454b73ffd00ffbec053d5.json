tChange: function(val) {\n        this.currentPage = val;\n        this.getData();\n    },\n    getData() {\n        var param = {\n            outfitName: this.query.outfitName, \n            uid2: this.type==='02'?this.userInfo.id:'',\n            pagesize: this.pagesize,\n            currentPage: this.currentPage,\n        };\n        this.$axios.post('/api/discuss/selectPage',param).then(res => {\n            if(res.data.code == 200){\n                this.tableData = res.data.data.list;\n                this.totalCount = res.data.data.total;\n            } else {\n                this.$message.warning(res.data.msg);\n            }\n        });\n    },\n    handleEdit(index, row) {\n        this.form = JSON.parse(JSON.stringify(row));\n        this.dialogVisible = true;\n        this.$refs['ruleForm'].clearValidate();\n        this.dialogName = \"코디 댓글 편집\";\n    },\n    handleAdd() {\n        if (this.$refs.rulform !== undefined) this.$refs.rulform.resetFields();\n        this.dialogVisible = true;\n        this.form = {};\n        this.$refs['ruleForm'].clearValidate();\n        this.dialogName = \"코디 댓글 추가\";\n    },\n    save(formName) {\n        this.$refs[formName].validate((valid) => {\n          if (valid) {\n            this.$axios.post(this.form.id?'/api/discuss/edit' : '/api/discuss/add', this.form).then(res => {\n              if(res.data.code == 200){\n                this.$message.success(res.data.msg);\n                this.dialogVisible = false;\n                this.getData();\n              } else {\n                this.$message.warning(res.data.msg);\n              }\n            })\n          } else {\n            console.log('error submit!!');\n            return false;\n          }\n        });\n    },\n    handleDelete(index, row) {\n        this.$confirm('삭제하시겠습니까?', '알림', {\n           type: 'warning'\n        }).then(() => {\n             this.$axios.get('/api/discuss/deleteById?id='+row.id).then(res => {\n                if(res.data.c